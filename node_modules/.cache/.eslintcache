a(sumEstData)\r\n                                                let sumRealData = realProgressData\r\n                                                sumRealData[material] = sumReal\r\n                                                await setRealProgressData(sumRealData)\r\n                                                let totalEst = estimatedMaterialData[estimated[i-1].material_id]\r\n                                                if(!totalEst){\r\n                                                    totalEst = 0\r\n                                                }\r\n                                                let diff = countEst - totalEst\r\n                                                let warning = null\r\n                                                if(totalEst < countEst){\r\n                                                    warning = <text className=\"warning__text\">*Estimated exceeded by {diff}!</text>\r\n                                                }\r\n                                                if(!totalEst){\r\n                                                    totalEst = \"(0 estimated)\"\r\n                                                }else{\r\n                                                    totalEst = \"(\" + totalEst.toString() + \" estimated)\"\r\n                                                }\r\n\r\n                                                let t_class = table_class[matIDList.indexOf(material)]\r\n\r\n                                                await tables.push(<div id=\"hot-app\" style={{borderBottom:\"1px solid lightgray\", width:\"1750px\", paddingBottom:\"30px\", marginTop:\"20px\"}}><div style={{display:\"flex\"}}><text className=\"materials__title\">{estimated[i-1].name.toUpperCase()} Isometrics <text style={{fontSize:\"17px\"}}>{totalEst}</text> {warning}</text><button className=\"save__button\" onClick={()=> submitEstimatedForecast(estimated[i-1].material_id)}><img src={SaveIcon} alt=\"save\" cl